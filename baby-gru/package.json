{
  "name": "moorhen",
  "version": "0.6.2",
  "private": false,
  "main": "moorhen.js",
  "types": "./types/main.d.ts",
  "homepage": "./",
  "author": "Paul Emsley, Filomeno Sanchez Rodriguez, Martin Noble, Stuart McNicholas",
  "description": "Moorhen molecular graphics",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moorhen-coot/Moorhen.git"
  },
  "bugs": {
    "url": "https://github.com/moorhen-coot/Moorhen/issues"
  },
  "keywords": [
    "molecular graphics",
    "moorhen",
    "webcoot",
    "ccp4",
    "interactive model building"
  ],
  "config": {
    "forge": {
      "packagerConfig": {
        "ignore": [
          "^/public/baby-gru$",
          "^/public/favicon.ico$",
          "^/public/index.html",
          "^/public/logo192.png",
          "^/public/manifest.json",
          "^/Dockerfile",
          "^/README.md",
          "^/SimpleCrossOriginServer.py",
          "^/cloud",
          "^/docker-compose.yml",
          "^/forge.config.js",
          "^/nginx",
          "^/nodemon.json",
          "^/out",
          "^/src$"
        ],
        "icon": "src/icons/icon",
        "name": "Moorhen"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Moorhen"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "linux"
          ]
        }
      ]
    }
  },
  "dependencies": { },
  "scripts": {
    "create-version": "npx genversion -e -p ./package.json -d -s ./src/version.js",
    "transpile-ts-worker": "npx tsc --module es2015 --lib webworker --skipLibCheck public/baby-gru/CootWorker.ts && sed -i.bak '/export {}/d' public/baby-gru/CootWorker.js && rm public/baby-gru/CootWorker.js.bak",
    "transpile-ts-worker-jest": "npx tsc --module es2015 --lib webworker --skipLibCheck public/baby-gru/CootWorker.ts && sed -i.bak 's/export {}/export { doCootCommand, setModules }/g' public/baby-gru/CootWorker.js && sed -i.bak '/importScripts.*;/d' public/baby-gru/CootWorker.js && sed -i.bak 's/onmessage/function setModules(arg0, arg1) { molecules_container = arg0; cootModule = arg1 }; let postMessage = () => {}; let onmessage/g' public/baby-gru/CootWorker.js && rm public/baby-gru/CootWorker.js.bak",
    "prestart": "npm run create-version && npm run transpile-ts-worker",
    "start": "vite",
    "prebuild": "npm run create-version && npm run transpile-ts-worker",
    "build": "vite build",
    "build-release": "npm run prebuild && webpack --mode production --devtool inline-source-map",
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron-forge start",
    "package": "npm run build && electron-forge package",
    "make-mac": "npm run build && electron-forge make --platform darwin",
    "make-linux": "npm run build && electron-forge make --platform linux",
    "make-win32": "npm run build && electron-forge make --platform win32",
    "create-docs": "npm run create-version && npx jsdoc -c jsdoc.json",
    "test": "npm run create-version && npx tsc && npm run transpile-ts-worker-jest && ln -s ./public/baby-gru/wasm/moorhen.data ./moorhen.data && node --trace-warnings --experimental-vm-modules --experimental-wasm-threads node_modules/jest/bin/jest.js --forceExit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@electron-forge/cli": "^6.0.5",
    "@electron-forge/maker-deb": "^6.0.5",
    "@electron-forge/maker-rpm": "^6.0.5",
    "@electron-forge/maker-squirrel": "^6.0.5",
    "@electron-forge/maker-zip": "^6.0.5",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.9",
    "@mui/lab": "^5.0.0-alpha.140",
    "@mui/material": "^5.10.10",
    "@mui/x-tree-view": "^6.17.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^18.16.14",
    "@types/react": "^18.2.6",
    "@vitejs/plugin-react-swc": "^3.4.1",
    "acorn": "^8.10.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^9.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-app": "^3.1.2",
    "bootstrap": "^5.1.3",
    "chart.js": "^3.7.1",
    "chartjs-plugin-annotation": "^2.1.2",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.2",
    "d3": "^5.15.0",
    "electron": "^23.1.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.2",
    "genversion": "^3.1.1",
    "gl-matrix": "^3.4.3",
    "html-react-parser": "^1.4.14",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "jsdoc": "^4.0.2",
    "localforage": "^1.10.0",
    "mini-css-extract-plugin": "^2.7.6",
    "nested-worker": "git+https://github.com/johanholmerin/nested-worker.git#semver:^1.0.0",
    "nightingale-linegraph-track": "^3.8.10",
    "node-fetch": "^2.7.0",
    "nodemon": "^2.0.22",
    "pako": "^2.0.4",
    "parcel-bundler": "^1.12.5",
    "pca-js": "^1.0.2",
    "plotly.js": "^2.12.1",
    "prismjs": "^1.29.0",
    "prop-types": "^15.8.1",
    "protvista-manager": "^3.4.0",
    "protvista-navigation": "^3.5.1",
    "protvista-sequence": "^3.5.1",
    "protvista-track": "^3.4.0",
    "rc-tree": "4.2.2",
    "re-resizable": "^6.9.11",
    "react": "^18.1.0",
    "react-bootstrap": "^2.3.1",
    "react-bootstrap-typeahead": "^6.0.0-alpha.11",
    "react-chartjs-2": "^4.1.0",
    "react-color": "^2.19.3",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.1.0",
    "react-draggable": "^4.4.5",
    "react-plotly.js": "^2.5.1",
    "react-redux": "^8.1.3",
    "react-simple-code-editor": "^0.13.1",
    "reactcss": "^1.2.3",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.9",
    "taffydb": "^2.7.3",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.0.0",
    "util": "^0.12.5",
    "uuid": "^9.0.0",
    "vite": "^4.5.0",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-cross-origin-isolation": "^0.1.6",
    "vite-plugin-top-level-await": "^1.3.1",
    "vite-plugin-wasm": "^3.2.2",
    "wait-on": "^7.0.1",
    "web-vitals": "^2.1.4",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-node-externals": "^3.0.0",
    "worker-loader": "^3.0.8"
  }
}
